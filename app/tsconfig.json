{
    "compilerOptions": {
        "outDir": "dist",
        "noEmit": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "skipDefaultLibCheck": true,
        "noImplicitAny": false
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
}
